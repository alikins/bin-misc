#!/bin/bash

#GIT_REPOS="subscription-manager python-rhsm candlepin"
GIT_REPOS="ansible ansible-modules-core ansible-modules-extras ansible-setup bin-misc dotfiles-shared dotfiles-private"

for GIT_REPO in $GIT_REPOS ; 
do 
    echo "${GIT_REPO}"
    git --git-dir="${HOME}/src/${GIT_REPO}/.git/" "$@" yesterday
done

